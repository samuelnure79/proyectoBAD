<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.OracleDialect</property>
    <property name="hibernate.connection.driver_class">oracle.jdbc.OracleDriver</property>
    <property name="hibernate.connection.url">jdbc:oracle:thin:@localhost:1521:orcl</property>
    <property name="hibernate.connection.username">grupo8</property>
    <property name="hibernate.connection.password">grupo8</property>
    <property name="hibernate.current_session_context_class">thread</property>
    <mapping class="model.Persona"/>
    <mapping class="model.Devolucion"/>
    <mapping class="model.DetalleDevolucion"/>
    <mapping class="model.DetalleDeOrdenCompra"/>
    <mapping class="model.DetalleRequisicion"/>
    <mapping class="model.Pedido"/>
    <mapping class="model.Proveedor"/>
    <mapping class="model.Requisicion"/>
    <mapping class="model.Producto"/>
    <mapping class="model.Empleado"/>
    <mapping class="model.DetalleProducto"/>
    <mapping class="model.Transaccion"/>
    <mapping class="model.Bodega"/>
    <mapping class="model.Cliente"/>
    <mapping class="model.Cotizacion"/>
    <mapping class="model.Inventario"/>
    <mapping class="model.Usuario"/>
    <mapping class="model.Catalogo"/>
    <mapping class="model.Telefono"/>
    <mapping class="model.DetallePedido"/>
    <mapping class="model.TipoCatalogo"/>
    <mapping class="model.OrdenDeCompra"/>
    <mapping class="model.HibernateUtil"/>
  </session-factory>
</hibernate-configuration>
